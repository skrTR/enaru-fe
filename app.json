{
  "expo": {
    "name": "enaru",
    "slug": "enaru",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/enau/aav4.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/enau/splash3.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.enaru.ngo",
      "buildNumber": "0.0.2",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },

    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Access to your photo library is required to attach photos to  event images upload."
        }
      ]
    ],
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/enau/aav2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "org.enaru.ngo",
      "permissions": ["CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
